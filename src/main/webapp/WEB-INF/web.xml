<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>jsp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>ADMIN</param-name>
    <param-value>KBH</param-value>
  </context-param>
  <servlet>
    <servlet-name>basicSevlet</servlet-name>
    <servlet-class>kr.or.ddit.servelt.BasicSevlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>basicSevlet</servlet-name>
    <url-pattern>/bs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>nowDate</servlet-name>
    <servlet-class>kr.or.ddit.servlet.NowDate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>nowDate</servlet-name>
    <url-pattern>/nowDate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>gugudan</servlet-name>
    <servlet-class>kr.or.ddit.servlet.Gugudan</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>gugudan</servlet-name>
    <url-pattern>/timesTables</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500/500.html</location>
  </error-page>
  <filter>
    <filter-name>loggingFilter</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>loggingFilter2</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter2</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter2</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>requestParameterEncodingFilter</filter-name>
    <filter-class>kr.or.ddit.filter.RequestParameterEncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>requestParameterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>defaultParamFilter</filter-name>
    <filter-class>kr.or.ddit.filter.DefaultParamFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>defaultParamFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>kr.or.ddit.listener.SessionListener</listener-class>
  </listener>
  <listener>
    <listener-class>kr.or.ddit.listener.SessionAttributeListener</listener-class>
  </listener>
  <listener>
    <listener-class>kr.or.ddit.listener.ApplicationListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>BasicJasonSevlet</display-name>
    <servlet-name>BasicJasonSevlet</servlet-name>
    <servlet-class>kr.or.ddit.servlet.BasicSevlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BasicJasonSevlet</servlet-name>
    <url-pattern>/BasicJasonSevlet</url-pattern>
  </servlet-mapping>
</web-app>